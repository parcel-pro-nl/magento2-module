{
  "name": "parcelpro/shipment",
  "description": "Verzendmodule om gemakkelijk orders in te laden in het verzendsysteem van Parcel Pro.",
  "version": "2.17.0",
  "keywords": [
    "verzenden",
    "pakket",
    "verzendopties",
    "postnl",
    "dhl"
  ],
  "type": "magento2-module",
  "require": {
    "php": ">=7.1"
  },
  "license": [
    "OSL-3.0"
  ],
  "authors": [
    {
      "name": "Parcel Pro",
      "email": "ict@parcelpro.nl",
      "homepage": "https://www.parcelpro.nl",
      "role": "Developer"
    }
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Parcelpro\\Shipment\\": ""
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "magento/community-edition": "^2.4",
    "phpmd/phpmd": "^2.14"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false,
      "magento/magento-composer-installer": false,
      "magento/composer-dependency-version-audit-plugin": false
    }
  },
  "scripts": {
    "cs:check": "phpcs",
    "cs:fix": "phpcbf",
    "md": "phpmd . ansi phpmd.xml --exclude vendor",
    "md:github": "phpmd . github phpmd.xml --exclude vendor"
  }
}
