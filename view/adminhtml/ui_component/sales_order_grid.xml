<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="send_to_parcelpro">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">send_to_parcelpro</item>
                        <item name="id" xsi:type="string">send_to_parcelpro</item>
                        <item name="label" xsi:type="string" translate="true">Zending aanmelden</item>
                        <item name="url" xsi:type="url" path="pp_shipment/shipment/index"/>
                    </item>
                </argument>
            </action>
            <action name="printlabels_of_parcelpro">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">printlabels_of_parcelpro</item>
                        <item name="id" xsi:type="string">printlabels_of_parcelpro</item>
                        <item name="label" xsi:type="string" translate="true">Zendinglabels afdrukken</item>
                        <item name="url" xsi:type="url" path="pp_shipment/shipment/printlabel"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>